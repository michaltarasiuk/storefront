{
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "type-check": "next typegen && tsc --noEmit --incremental false",
    "lint": "eslint",
    "lint-fix": "eslint --fix .",
    "ci": "bun run prettier-check && bun run type-check && bun run lint && bun run build",
    "extract": "formatjs extract --out-file messages/extracted/en-US.json -- '!**/*.d.ts' '**/*.{ts,tsx}' && prettier --write messages/extracted/en-US.json",
    "compile-folder": "formatjs compile-folder --ast messages/extracted messages/compiled && prettier --write messages/compiled",
    "extract-compile": "bun run extract && bun run compile-folder",
    "generate": "graphql-codegen --config codegen.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/client-integration-nextjs": "^0.12.2",
    "@formatjs/intl-localematcher": "^0.6.2",
    "@t3-oss/env-nextjs": "^0.13.10",
    "client-only": "^0.0.1",
    "cva": "npm:class-variance-authority",
    "graphql": "^16.12.0",
    "jsonwebtoken": "^9.0.3",
    "negotiator": "^1.0.0",
    "next": "^16.1.1",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.2.3",
    "react-aria": "^3.45.0",
    "react-aria-components": "^1.14.0",
    "react-dom": "^19.2.3",
    "react-intl": "^7.1.14",
    "react-stately": "^3.43.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-react-aria-components": "^2.0.1",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/eslintrc": "^3.3.3",
    "@formatjs/cli": "^6.9.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/env": "16.0.0",
    "@parcel/watcher": "^2.5.1",
    "@react-types/shared": "^3.32.1",
    "@storybook/addon-a11y": "^9.1.17",
    "@storybook/addon-docs": "^9.1.17",
    "@storybook/addon-onboarding": "^9.1.17",
    "@storybook/nextjs-vite": "^9.1.17",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.0",
    "eslint-plugin-formatjs": "^5.4.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.17",
    "graphql-config": "^5.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss-load-config": "^6.0.1",
    "prettier": "3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^9.1.17",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
