{
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "start": "next start",
    "build": "next build",
    "ci": "bun run prettier-check && bun run type-check && bun run lint && bun run test && bun run build",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "type-check": "next typegen && tsc --noEmit --incremental false",
    "lint": "next lint",
    "lint-fix": "eslint --fix .",
    "test": "vitest run",
    "extract": "formatjs extract --out-file messages/extracted/en-US.json -- '!**/*.d.ts' '**/*.{ts,tsx}' && prettier --write messages/extracted/en-US.json",
    "compile-folder": "formatjs compile-folder --ast messages/extracted messages/compiled && prettier --write messages/compiled",
    "extract-compile": "bun run extract && bun run compile-folder",
    "generate": "graphql-codegen --config codegen.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
