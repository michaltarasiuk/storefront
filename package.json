{
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "ci": "bun run build && bun run lint && bun run prettier-check",
    "lint": "next lint",
    "lint-fix": "eslint --fix .",
    "prettier-write": "prettier --write",
    "prettier-check": "prettier --check .",
    "format": "bun run prettier write .",
    "extract": "formatjs extract --out-file src/i18n/messages/extracted/en-US.json -- '!**/*.d.ts' '**/*.{ts,tsx}' && bun run prettier-write src/i18n/messages/extracted/en-US.json",
    "compile-folder": "formatjs compile-folder --ast src/i18n/messages/extracted src/i18n/messages/compiled && bun run prettier-write src/i18n/messages/compiled",
    "extract-compile": "bun run extract && bun run compile-folder"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "class-variance-authority": "^0.7.1",
    "negotiator": "^1.0.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.0.0",
    "react-intl": "^7.1.11",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-react-aria-components": "^2.0.0",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@formatjs/cli": "^6.7.2",
    "@tailwindcss/postcss": "^4",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-formatjs": "^5.4.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss-load-config": "^6.0.1",
    "prettier": "3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
